<!DOCTYPE html>
<html lang="es">

<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>
        <%= title %>
    </title>
</head>

<body>
    <h1>
        <%= title %>
    </h1>

    <ul>
        <% posts.forEach(function(post){ %>
            <li>
                <p><span>
                        <%= post.id %>)
                    </span>
                    <%= post.title %>
                </p>
                <p><%= post.description %></p>
                <p><%= post.author %></p>
            </li>

            <% }); %>
    </ul>

</body>

</html>